{
  "name": "bigquerysqlformatter",
  "displayName": "BigQuerySQLFormatter",
  "description": "VS Code extension providing BigQuery SQL syntax checking",
  "author": "sean-conkie",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/sean-conkie/BigQuerySQLFormatter-extension.git"
  },
  "publisher": "sean-conkie",
  "categories": [
    "Other"
  ],
  "keywords": [
    "multi-root ready",
    "bigquery",
    "sql"
  ],
  "engines": {
    "vscode": "^1.86.0"
  },
  "activationEvents": [
    "onLanguage:sql"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "bigquerysqlformatter.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.86.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9"
  }
}
